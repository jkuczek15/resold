{
    "name": "magento/project-community-edition",
    "description": "Buy & sell with same-day delivery",
    "type": "project",
    "version": "2.2.0",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "composer/composer": "@alpha",
        "magento/product-community-edition": "2.2.0",
        "magento/module-sample-data": "100.2.0",
        "aws/aws-sdk-php": "^3.64",
        "stripe/stripe-php": "^6.15",
        "mageplaza/module-delete-orders": "^1.1",
        "zsoerenm/magento2-optionaltelephone": "^1.4",
        "magehost/performance-dashboard": "^1.14",
        "mobiledetect/mobiledetectlib": "^2.8",
        "swissup/tippyjs": "^1.0",
        "elasticsearch/elasticsearch": "~6.0",
        "google/recaptcha": "^1.2",
        "sinergi/browser-detector": "^6.1",
        "msp/userlockout": "^1.1",
        "msp/shield": "^2.1",
        "msp/nospam": "^1.3",
        "msp/adminrestriction": "^1.3",
        "paquettg/php-html-parser": "^1.7",
        "guzzlehttp/guzzle": "^6.3",
        "psr/simple-cache": "^1.0",
        "kreait/firebase-php": "4.35",
        "grpc/grpc": "^1.27.0",
        "google/cloud-firestore": "^1.18"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.0",
        "squizlabs/php_codesniffer": "1.5.3",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.4.0",
        "lusitanian/oauth": "~0.3 <=0.7.0",
        "sebastian/phpcpd": "2.0.0"
    },
    "config": {
        "use-include-path": true
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": [
                "app/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "zorn_optionaltelephone": {
            "type": "vcs",
            "url": "git@github.com:zsoerenm/magento2-optionaltelephone.git"
        },
        "niks-multiple-layered-navigation": {
            "type": "vcs",
            "url": "git@github.com:NikZh/magento2-multiple-layered-navigation.git"
        }
    },
    "extra": {
        "magento-force": "override"
    }
}
