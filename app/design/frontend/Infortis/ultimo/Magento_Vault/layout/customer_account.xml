<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
      <referenceBlock name="customer_account_navigation">
          <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-my-credit-cards-link">
              <arguments>
                  <argument name="path" xsi:type="string">https://resold.us/api/stripe/dashboard</argument>
                  <argument name="label" xsi:type="string" translate="true">Stripe Dashboard</argument>
                   <argument name="attributes" xsi:type="array">
                    <item name="target" xsi:type="string">_blank</item>
                  </argument>
              </arguments>
          </block>
      </referenceBlock>
    </body>
</page>
