<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Store/etc/config.xsd">
    <default>
        <payment>
            <ced_csstripe_method_one>
                <active>1</active>
                <title>CEDCOMMERCE STRIPE PAYMENT</title>
                <model>Ced\CsStripePayment\Model\Paymethod</model>
                <new_order_status>processing</new_order_status>
                <payment_action>authorize_capture</payment_action>
            </ced_csstripe_method_one>
        </payment>
        <ced_csmarketplace>
          <vendor>
				<payment_methods>
					<stripe>*</stripe>
				</payment_methods>
			</vendor>
        </ced_csmarketplace>
    </default>
</config>
