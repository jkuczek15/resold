<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Plumrocket_GDPR::css/prgdpr.css"/>
        <css src="Plumrocket_GDPR::css/prgdpr-custom.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Plumrocket\GDPR\Block\Account\Index" name="prgdpr_index" cacheable="false" template="Plumrocket_GDPR::account/index.phtml">
                <block class="Magento\Cms\Block\Block" name="privacy_faqs">
                    <arguments>
                        <argument name="block_id" xsi:type="string">prgdpr_privacy_faqs</argument>
                    </arguments>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
